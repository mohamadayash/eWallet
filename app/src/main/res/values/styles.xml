<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <!-- Customize your theme here. -->
    </style>

    <style name="MyCustomToolBar" parent="ThemeOverlay.AppCompat.Dark">
        <item name="android:listChoiceBackgroundIndicator">@drawable/abc_btn_check_material</item>
        <item name="android:radioButtonStyle">@drawable/abc_btn_radio_material</item>
    </style>

    <style name="MyDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:textColor">@color/green</item>
    </style>

    <style name="PagerTabStripText">
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/primaryColor</item>
    </style>

</resources>
